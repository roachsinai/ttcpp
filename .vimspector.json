// https://github.com/puremourning/vimspector/tree/master/support/test
{
  "configurations": {
    "gdb: launch": {
      "adapter": "vscode-cpptools",
      "configuration": {
        "name": "gdb: Launch",
        "type": "cppdbg",
        "request": "launch",
        "justMyCode": false,
        "program": "build/Debug/ready",
        "cwd": "${workspaceRoot}",
        "console": "integratedTerminal",
        "miDebuggerPath": "/usr/bin/gdb",
        "MIMode": "gdb"
      }
    }
  }
}
